https = game:GetService("HttpService")

body = https:JSONEncode({

	["content"] = "",

	["embeds"] = {

		["title"] = "## SCware execution ##",

		["descriptrion"] = game.Players.LocalPlayer.Name .. " has executed SCware.",

		["color"] = tonumber(000000),

		["fields"] = {

			["time"] = "Time:",
			["TValue"] = os.date(), os.time(),


			["profile"] = "profile:",
			["PValue"] = "https://www.roblox.com/users/".. game.Players.LocalPlayer.UserId .."/profile",


			["game"] = "game:",
			["GValue"] = "https://www.roblox.com/games/" .. game.GameId,

			
			["Hardware"] = "Hardware ID:",
			["HValue"] = game:GetService("RbxAnalyticsService"):GetClientId(),

			["inline"] = true
		}
	}
})

response = request({
	Url = "https://discord.com/api/webhooks/1435519726033047603/L0qHy5q5Ir_QIu9DyvsDFRtb-C4A1EC3wZVXHk8Lkp0lPn16mv3k1HKalYjLAn68BQhc",
	Method = "POST",
	Body = body
})
